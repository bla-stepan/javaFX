<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import java.net.URL?>
<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="JavaFX_511.FXMLExample.FXMLDocumentController"
      prefHeight="300.0" prefWidth="300.0" spacing="10" alignment="CENTER">
    <children>
        <Label text="label" alignment="CENTER" prefHeight="70" prefWidth="100" fx:id="lable"/>
        <ToggleButton onAction="#handlePush" prefHeight="70" prefWidth="100" text="Нажать" fx:id="tbtn"/>
    </children>

    <stylesheets>
        <URL value="@style.css"/>
    </stylesheets>

</VBox>
